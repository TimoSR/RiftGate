# This is automatically set when deployed on the cloud.
GOOGLE_CLOUD_PROJECT=

# Service Name
##########################################
SERVICE_NAME=x-service

# URL FOR SERVICE HOST (Should end without a '/')
# This is unique for every service
##########################################
HOST_URL=
#HOST_URL=https://riftgate.ngrok.io
#HOST_URL=https://x-service-s4qgcf5egq-ew.a.run.app

# Environment: Development, Production
##########################################
ENVIRONMENT=Development

# JWT KEY
##########################################
JWT_KEY=
JWT_ISSUER=$SERVICE_NAME
JWT_AUDIENCE=

# Database Config
##########################################
# This is a secret
MONGODB_CONNECTION_STRING=

# Pubsub Emulator Settings
##########################################
# This is only used on local development, and is auto called by the library.
PUBSUB_EMULATOR_HOST=localhost:8085

# Redis
##########################################
#REDIS_CONNECTION_STRING=localhost:6379
REDIS_CONNECTION_STRING=